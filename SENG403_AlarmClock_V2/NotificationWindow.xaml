<Window x:Class="SENG403_AlarmClock_V2.NotificationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SENG403_AlarmClock_V2"
        mc:Ignorable="d"
        WindowStyle="None"
        Background="Black"
        Title="Window1" Height="200" Width="300">
    <Grid>
        <Button x:Name="dismissButton" Content="Dismiss" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="68,107,0,0" Click="dismissButton_Click"/>
        <Button x:Name="snoozeButton" Content="Snooze" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="177,107,0,0" Click="snoozeButton_Click"/>
        <Label x:Name="label" Content="Alarm has gone off!" Foreground="White" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="68,42,0,0" RenderTransformOrigin="0.5,0.5" FontSize="20"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="214,269,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
